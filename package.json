{
    "name": "yproject_policy",
    "version": "0.0.3",
    "description": "Local package manager. A tool to manage your multi-repo.",
    "type": "commonjs",
    "main": "./bin/index.js",
    "keywords": [
        "pnpm",
        "multi-repo"
    ],
    "bin": {
        "yproject_policy": "./bin/index.js"
    },
    "files": [
        "/bin",
        "/lib",
        "index.js",
        "index.cjs",
        "index.mjs",
        "README.md",
        "LICENSE",
        "Unlicense",
        "Unlicense.txt"
    ],
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch:src": "rmdir lib\\src /s /q & ycplmon2 fix src && inprint && babel src --config-file ./babel.lo.config.cjs --out-dir lib\\src --extensions \".ts\" --source-maps inline -w",
        "build": "rmdir lib\\src /s /q & ycplmon2 fix src && inprint && babel src --config-file ./babel.lo.config.cjs --out-dir lib\\src --extensions \".ts\" --source-maps inline",
        "republish": "npm run build && npx version-select && npx genversion --es6 --semi version.js && npm publish && npm i -g yproject_policy@latest && yproject_policy --version",
        "start": "node --inspect ./bin/index.js",
        "build:validators": "npx typescript-json-validator src/types/api/AckPacket.ts --useNamedExport"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yuyaryshev/yproject_policy.git"
    },
    "author": "Yuri Yaryshev",
    "license": "Unlicense",
    "bugs": {
        "url": "https://github.com/yuyaryshev/yproject_policy/issues"
    },
    "homepage": "https://github.com/yuyaryshev/yproject_policy#readme",
    "devDependencies": {
        "@babel/cli": "7.12.8",
        "@babel/core": "7.12.9",
        "@babel/node": "^7.12.6",
        "@babel/parser": "7.12.7",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-decorators": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/plugin-proposal-optional-chaining": "7.12.7",
        "@babel/plugin-transform-modules-commonjs": "^7.12.1",
        "@babel/plugin-transform-typescript": "^7.12.1",
        "@types/dir-glob": "^2.0.0",
        "@types/express": "^4.17.12",
        "@types/fs-extra": "^9.0.11",
        "@types/inquirer": "^7.3.1",
        "@types/micromatch": "^4.0.1",
        "@types/node": "^14.14.16",
        "babel-plugin-inline-replace-variables": "^1.3.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-watch": "^7.0.0",
        "json5": "^2.2.0",
        "prettier": "^2.0.5",
        "ts-json-schema-generator": "^0.93.0",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@mojotech/json-type-validation": "^3.1.0",
        "ajv": "^8.5.0",
        "array-union": "^2.1.0",
        "chalk": "^4.1.0",
        "commander": "^7.0.0",
        "deepmerge": "^4.2.2",
        "dir-glob": "^3.0.1",
        "execa": "^5.0.0",
        "express": "^4.17.1",
        "fs-extra": "^10.0.0",
        "globby": "^11.0.2",
        "inquirer": "^7.3.3",
        "inquirer-table-prompt": "^0.2.1",
        "micromatch": "^4.0.2"
    }
}